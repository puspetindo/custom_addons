<odoo>
    <data noupdate="0">
        <record id="seq_product_kode_barang" model="ir.sequence">
            <field name="name">Kode Barang Produk</field>
            <field name="code">product.template.kode.barang</field>
            <field name="prefix"></field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>


        <record id="seq_laporan_penerimaan_barang" model="ir.sequence">
            <field name="name">LPB Sequence</field>
            <field name="code">laporan.penerimaan.barang</field>
            <field name="prefix"></field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>

</odoo>
